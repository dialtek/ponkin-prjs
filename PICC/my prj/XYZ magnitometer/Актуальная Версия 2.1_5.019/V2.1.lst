....................  U16 rom_wr_CRC16 (ee_addr, U8 crc_buf) 
....................  { 
....................     U16 CRC_Wr; 
....................      
....................     CRC_Wr = CRC16 (crc_buf, 14); // ðàñ÷åò CRC16 
....................      
....................     rom_wr_byte (ee_addr, (CRC_Wr >> 8)); // çàïèñü ñò. áàéòà CRC â rom 
....................     ee_addr++;  // èíêðåìåíò àäðåñà 
....................     rom_wr_byte (ee_addr, (U8) CRC_Wr); // çàïèñü ìë. áàéòà CRC â rom 
....................      
....................     return CRC_Wr; 
....................  } 
....................  
....................  U16 rom_rd_CRC16 (ee_addr) 
....................  { 
....................     U8 CRC_rd_MSB; // ñò. áàéò CRC äëÿ çàïèñè 
....................     U8 CRC_rd_LSB; // ìë. áàéò CRC äëÿ çàïèñè 
....................     U16 CRC_rd; // 16 - áèò. CRC 
....................      
....................     CRC_rd_MSB = rom_rd_byte (ee_addr); // ÷òåíèå ñò. áàéòà CRC èç rom 
....................     ee_addr++; 
....................     CRC_rd_LSB = rom_rd_byte (ee_addr); // ÷òåíèå ìë. áàéòà CRC èç rom 
....................     CRC_rd = CRC_rd_MSB << 8; // âîññòàíîâëåíèå CRC 
....................     CRC_rd |= CRC_rd_LSB; // èç ïðî÷èòàííûõ áàéò 
....................      
....................     return CRC_rd; 
....................  } 
