        <xctk:IntegerUpDown x:Name="GDK_6_U" TabIndex="11" HorizontalAlignment="Left" Margin="111,275,0,0" VerticalAlignment="Top" Width="75" FontSize="16" Maximum="40" Minimum="0" ParsingNumberStyle="Integer" RenderTransformOrigin="0,0" TextAlignment="Center" DefaultValue="0" DisplayDefaultValueOnEmptyText="True" Text="\"/>
