        <Label x:Name="label_VDK_FI" Content="ВДК-ФИ" HorizontalAlignment="Left" Margin="12,61,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="18" Width="84" IsEnabled="False"/>
